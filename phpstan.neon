includes:
    - vendor/landingi/php-coding-standards/phpstan.neon

parameters:
    reportUnmatchedIgnoredErrors: false
    ignoreErrors:
        - '#Property [a-zA-Z0-9\\]+::[a-zA-Z0-9\\\$]+ is never read, only written.#'
        - '#Property [a-zA-Z0-9\\]+::[a-zA-Z0-9\\\$]+ is never written, only read.#'
